// cSpell Settings
{
  // Version of the setting file.  Always 0.2
  "version": "0.2",
  // language - current active spelling language
  "language": "en-AU, csharp",
  // words - list of words to be always considered correct
  "words": [],
  "ignorePaths": ["**/node_modules/**", "**/dist/**", "**/bin/**", "**/obj/**"],
  "enableFiletypes": ["csharp", "terraform", "json", "jsonc", "markdown", "mdx"]
}
